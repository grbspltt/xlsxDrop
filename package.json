{
  "name": "xlsx-drop",
  "version": "0.0.1",
  "description": "Test drop for xlsx and transpile es6 to es5 on server and client",
  "main": "index.js",
  "scripts": {
    "buildServer": "set NODE_ENV=production&& babel.cmd server\\src --out-dir server\\build",
    "buildClient": "webpack -p",
    "start":"node server\\build\\app.js",
    "startDev":"webpack-dev-server --devtool eval -content-base \\client\\build --progress --colors --hot --inline --history-api-fallback"
  },
  "author": "ks",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^2.10.0",
    "express": "^4.13.3",
    "flux": "^2.1.1",
    "ksplitt": "0.0.9",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1"
  },
  "devDependencies":{
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "webpack": "^1.12.1",
    "webpack-dev-server":"1.10.1"
  }
}
